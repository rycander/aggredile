<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title toggle-on">
      <% if (!entry.get('entry_visit_id')) { %>
        <ul class='list-inline unread'>
      <% } else { %>
        <ul class='list-inline'>
      <% }%>
        <li><%= entry.escape('title') %></li>
        <li class='pull-right clearfix'>
          <%= title %> - 
          <%= entry.escape('published_at') %>
        </li>
      </ul>
    </h3>
  </div>
</div>
